{
  "name": "<%- appname %>",
  "version": "1.0.0",
  "description": "An application based on Knockout and DevExtreme",
  "devDependencies": {
<% if (localization === 'globalize') { -%>
    "cldr-data": "^31.0.2",
    "globalize": "^1.3.0",
<% } else if (localization === 'intl') { -%>
    "devextreme-intl": "~17.1.4",
<% } -%>
    "jquery": "^3.1.1",
    "knockout": "^3.4.2",
    "devextreme": "~17.1.6",
    "css-loader": "^0.28.5",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "clean-webpack-plugin": "^0.1.16",
    "webpack-merge": "^4.1.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --open --config webpack.dev.js"
  }
}
